{
    "prefix": {
        "markdown": "(markdown) => ",
        "code": "(code, language) => ",
        "blockquote": "(quote) => ",
        "html": "(html) => ",
        "heading": "(text, level) => ",
        "hr": "() => ",
        "list": "(body, ordered) => ",
        "listitem": "(text) => ",
        "paragraph": "(text) => ",
        "table": "(header, body) => ",
        "tablerow": "(content) => ",
        "tablecell": "(content, header, align) => ",
        "strong": "(text) => ",
        "em": "(text) => ",
        "codespan": "(code) => ",
        "br": "() => ",
        "del": "(text) => ",
        "link": "(href, title, text) => ",
        "image": "(href, title, text) => ",
        "text": "(text) => "
    },
    "suffix_default": {
        "markdown": "${(s=>{let e=document.createElement('textarea');e.innerHTML=s;return e.value})(markdown.split('[br]').join('')).replace(/®(.+?)®(.+?)®/g, '[ruby=$2]$1[/ruby]').replace(/å([\\S\\s]*?)å/g, '[spoiler][size=3]$1[/size][/spoiler]\\n').replace(/ß([\\S\\s]*?)ß/g, '[code]$1[/code]\\n')}",
        "code": "[markdown]\\`\\`\\`${language}\\n${code}\\n\\`\\`\\`[/markdown]\\n",
        "blockquote": "[quote]${quote.replace('[br]', '').replace(/\\n+$/,'')}[/quote]\\n",
        "html": "${((d,s)=>d.createElement('span').appendChild(d.createTextNode(s)).parentNode.innerHTML)(document,html)}",
        "heading": "${level==2?'\\n':''}[size=${7-level}]${text}[/size]\\n\\n",
        "hr": "[hr]\\n",
        "list": "[list${ordered?'=1':''}]\\n${body.replace(/\\n+$/,'')}\\n[/list]\\n",
        "listitem": "[*]${text.replace(/\\n+$/,'')}\\n",
        "paragraph": "${text.split('\\n').join('[br]').split('[br][br]').join('\\n')}\\n",
        "table": "[table=50%]\\n${header}${body}[/table]\\n",
        "tablerow": "[tr]${content}[/tr]\\n",
        "tablecell": "[td]${content}[/td]",
        "strong": "[b]${text}[/b]",
        "em": "[i]${text}[/i]",
        "codespan": "[markdown]\\`${code}\\`[/markdown]",
        "br": "[br][br]",
        "del": "[s]${text}[/s]",
        "link": "[url=${href}]${text}[/url]",
        "image": "[img]${href}[/img]\\n",
        "text": "${text.replace('[br]','\\n').replace(/Ω/, '[page]\\n').replace(/≈/, '\\n\\nPart题：\\n')}"
    }
}
